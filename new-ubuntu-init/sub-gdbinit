#/* vim: set filetype=gdb : */

define debug
target remote localhost:2331
monitor reset
monitor halt
load
delete
quit
end

define ll
target remote localhost:2331
load
delete
end

define reset
target remote localhost:2331
load
delete
b qspi_test
b qspi_test_end
c
end

define resetc
target remote localhost:2331
monitor reset
monitor halt
load
delete
b qspi_test_end
c
end

define fs
focus src
end

define fc
focus cmd
end

define fa
focus asm
end

define qspi
x/45xw 0x2f100000
end
